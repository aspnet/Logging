{
  "version": "1.1.1",
  "packOptions": {
    "repository": {
      "type": "git",
      "url": "git://github.com/aspnet/logging"
    },
    "tags": [
      "logging"
    ]
  },
  "description": "Provides a common way to filter log messages across all registered logger providers.",
  "buildOptions": {
    "warningsAsErrors": true,
    "keyFile": "../../tools/Key.snk",
    "xmlDoc": true,
    "nowarn": [
      "CS1591"
    ]
  },
  "dependencies": {
    "Microsoft.Extensions.Configuration.Abstractions": "1.1.1",
    "Microsoft.Extensions.Logging.Abstractions": "1.1.1",
    "NETStandard.Library": "1.6.1"
  },
  "frameworks": {
    "netstandard1.1": {}
  }
}