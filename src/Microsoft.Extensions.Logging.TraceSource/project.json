        "System.Collections.Concurrent": ""
      }
    },
    "netstandard1.3": {
      "dependencies": {
        "System.Collections.Concurrent": "4.0.12-*",
        "System.Collections": "4.0.11-*",
        "System.Diagnostics.TraceSource": "4.0.0-*",
        "System.Globalization": "4.0.11-*",
        "System.Linq": "4.1.0-*",
        "System.Threading": "4.0.11-*"
      },
      "imports": [
        "dotnet5.4"
      ]
    }
  }
}