{
  "version": "1.0.0-*",
  "description": "TraceSource sink for Microsoft.Extensions.Logging.  This sinks logs messages to a trace listener by writing messages with System.Diagnostics.TraceSource.TraceEvent().",
  "tags": [
    "logging",
    "tracesource"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/aspnet/logging"
  },
  "compilationOptions": {
    "warningsAsErrors": true,
    "keyFile": "../../tools/Key.snk",
    "nowarn": [
      "CS1591"
    ],
    "xmlDoc": true
  },
  "dependencies": {
    "Microsoft.Extensions.Logging": "1.0.0-*"
  },
  "frameworks": {
    "net451": {
      "frameworkAssemblies": {
        "System.Runtime": { "type": "build" }
      }
    },
    "netstandard1.3": {
      "dependencies": {
        "System.Diagnostics.TraceSource": "4.0.0-*"
      },
      "imports": [
        "dotnet5.4"
      ]
    }
  }
}