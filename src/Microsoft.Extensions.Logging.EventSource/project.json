{
  "version": "1.1.1",
  "description": "EventSource/EventListener logger provider implementation for Microsoft.Extensions.Logging.",
  "packOptions": {
    "tags": [
      "EventSource",
      "logging",
      "ETW"
    ],
    "repository": {
      "type": "git",
      "url": "git://github.com/aspnet/logging"
    }
  },
  "buildOptions": {
    "warningsAsErrors": true,
    "keyFile": "../../tools/Key.snk",
    "xmlDoc": true
  },
  "dependencies": {
    "Microsoft.Extensions.Logging.Abstractions": "1.1.1",
    "NETStandard.Library": "1.6.1",
    "Newtonsoft.Json": "9.0.1"
  },
  "frameworks": {
    "net451": {
      "buildOptions": {
        "define": [
          "NO_EVENTSOURCE_COMPLEX_TYPE_SUPPORT"
        ]
      },
      "frameworkAssemblies": {
        "System.Runtime": {
          "type": "build"
        }
      }
    },
    "netstandard1.3": {}
  }
}