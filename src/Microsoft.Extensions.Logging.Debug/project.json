{
  "version": "1.0.0-*",
  "description": "Debug output sink implementation for Microsoft.Extensions.Logging. This sink logs messages to a debugger monitor by writing messages with System.Diagnostics.Debug.WriteLine().",
  "tags": [
    "logging"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/aspnet/logging"
  },
  "compilationOptions": {
    "warningsAsErrors": true,
    "keyFile": "../../tools/Key.snk",
    "nowarn": [
      "CS1591"
    ],
    "xmlDoc": true
  },
  "dependencies": {
    "Microsoft.Extensions.Logging": "1.0.0-*"
  },
  "frameworks": {
    "netstandard1.1": {
      "dependencies": {
        "System.Diagnostics.Debug": "4.0.11-*"
      },
      "imports": [
        "dotnet5.2"
      ]
    }
  }
}