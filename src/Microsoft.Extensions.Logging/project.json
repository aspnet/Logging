{
  "version": "1.0.1",
  "description": "Logging infrastructure default implementation for Microsoft.Extensions.Logging.",
  "packOptions": {
    "repository": {
      "type": "git",
      "url": "git://github.com/aspnet/logging"
    },
    "tags": [
      "logging"
    ]
  },
  "dependencies": {
    "Microsoft.Extensions.DependencyInjection.Abstractions": "1.0.1",
    "Microsoft.Extensions.Logging.Abstractions": "1.0.1"
  },
  "buildOptions": {
    "warningsAsErrors": true,
    "define": [
      "TRACE"
    ],
    "keyFile": "../../tools/Key.snk",
    "nowarn": [
      "CS1591"
    ],
    "xmlDoc": true
  },
  "frameworks": {
    "netstandard1.1": {
      "dependencies": {
        "System.Threading": "4.0.11"
      }
    }
  }
}