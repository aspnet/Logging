{
  "version": "1.0.2",
  "packOptions": {
    "repository": {
      "type": "git",
      "url": "git://github.com/aspnet/logging"
    },
    "tags": [
      "logging"
    ]
  },
  "description": "Console logger provider implementation for Microsoft.Extensions.Logging.",
  "buildOptions": {
    "warningsAsErrors": true,
    "keyFile": "../../tools/Key.snk",
    "nowarn": [
      "CS1591"
    ],
    "xmlDoc": true
  },
  "dependencies": {
    "System.Runtime.InteropServices.RuntimeInformation": "4.0.0",
    "Microsoft.Extensions.Configuration.Abstractions": "1.0.2",
    "Microsoft.Extensions.Logging.Abstractions": "1.0.2"
  },
  "frameworks": {
    "net451": {
      "frameworkAssemblies": {
        "System.Runtime": {
          "type": "build"
        }
      }
    },
    "netstandard1.3": {
      "dependencies": {
        "System.Console": "4.0.0",
        "System.Threading": "4.0.11"
      }
    }
  }
}