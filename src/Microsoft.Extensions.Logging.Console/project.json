{
  "version": "1.1.1",
  "packOptions": {
    "repository": {
      "type": "git",
      "url": "git://github.com/aspnet/logging"
    },
    "tags": [
      "logging"
    ]
  },
  "description": "Console logger provider implementation for Microsoft.Extensions.Logging.",
  "buildOptions": {
    "warningsAsErrors": true,
    "keyFile": "../../tools/Key.snk",
    "nowarn": [
      "CS1591"
    ],
    "xmlDoc": true
  },
  "dependencies": {
    "Microsoft.Extensions.Configuration.Abstractions": "1.1.1",
    "Microsoft.Extensions.Logging.Abstractions": "1.1.1",
    "NETStandard.Library": "1.6.1"
  },
  "frameworks": {
    "net451": {
      "frameworkAssemblies": {
        "System.Runtime": {
          "type": "build"
        }
      }
    },
    "netstandard1.3": {}
  }
}