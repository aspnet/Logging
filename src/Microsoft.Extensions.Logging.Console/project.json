{
  "version": "1.2.0-*",
  "packOptions": {
    "repository": {
      "type": "git",
      "url": "git://github.com/aspnet/logging"
    },
    "tags": [
      "logging"
    ]
  },
  "description": "Console logger provider implementation for Microsoft.Extensions.Logging.",
  "buildOptions": {
    "warningsAsErrors": true,
    "keyFile": "../../tools/Key.snk",
    "nowarn": [
      "CS1591"
    ],
    "xmlDoc": true
  },
  "dependencies": {
    "Microsoft.Extensions.Configuration.Abstractions": "1.2.0-*",
    "Microsoft.Extensions.Logging.Abstractions": {
      "target": "project"
    },
    "NETStandard.Library": "1.6.2-*"
  },
  "frameworks": {
    "net451": {
      "frameworkAssemblies": {
        "System.Runtime": {
          "type": "build"
        }
      }
    },
    "netstandard1.3": {},
    "netstandard1.5": {
      "dependencies": {
        "System.Runtime.Loader": "4.4.0-*"
      }
    }
  }
}