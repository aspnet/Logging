{
  "version": "1.0.0-*",
  "repository": {
    "type": "git",
    "url": "git://github.com/aspnet/logging"
  },
  "description": "Logging abstractions to be used by library authors.",
  "compilationOptions": {
    "warningsAsErrors": true,
    "keyFile": "../../tools/Key.snk",
